    <category>
        <pattern>HELLO</pattern>
        <template>
            Well, hello!
        </template>
    </category>

    <category>
        <pattern>YOUR PART IS SUFFICIENT I SAID</pattern>
        <template>
            Works!
        </template>
    </category>

    <category>
        <pattern>HI</pattern>
